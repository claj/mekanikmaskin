<!-- source: https://github.com/vaughnd/clojure-example-logback-integration/blob/master/src/logback-console-only.xml -->
<configuration debug="false">
 <appender name="MAIN" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level %-10contextName %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="mekanikmaskin" level="DEBUG"/>

  <root level="INFO">
    <appender-ref ref="MAIN"/>
  </root>
</configuration>
